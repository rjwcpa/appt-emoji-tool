<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" 
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
           xsi:type="MailApp">
  <Id>a1b2c3d4-e5f6-4789-9abc-def012345678</Id>
  <Version>1.0.0.20</Version>
  <ProviderName>Internal Tools</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Meeting Icons" />
  <Description DefaultValue="Add icons to appointment subjects." />
  <Hosts>
    <Host Name="Mail" />
  </Hosts>
  <FormSettings>
    <Form xsi:type="ItemEdit">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://rjwcpa.github.io/appt-emoji-tool/taskpane.html"/>
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteItem</Permissions>
  <Rule xsi:type="RuleCollection" Mode="And">
    <Rule xsi:type="ItemIs" ItemType="Appointment" FormType="Edit" />
  </Rule>
</OfficeApp>
